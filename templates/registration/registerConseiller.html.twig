{% extends 'base_admin.html.twig' %}
{% block title %}Inscription des conseillers{% endblock %}

{% block body %}

<h1 class="what_taital">Inscription des conseillers</h1>
<div class="contact_section2">
	<div class="row justify-content-center align-items-center h-100">
		<div class="col col-sm-6">
			<div class="mail_section">
				{{ form_start(registrationForm) }}
				{{ form_row(registrationForm.nom) }}
				{{ form_row(registrationForm.prenom) }}
				{{ form_row(registrationForm.email) }}
				{{ form_row(registrationForm.adresse) }}
				{{ form_row(registrationForm.telephone) }}
				{{ form_row(registrationForm.plainPassword, {
            label: 'Password'
        }) }}

				<button type="submit" class="btn btn-danger">l'inscrire</button>
				{{ form_end(registrationForm) }}
			</div>
		</div>
	</div>
</div>{% endblock %}

